COLUMN TABLE "ARTEC_PC_NOTIFICATION_EVENT_DEST" (
  ID NVARCHAR(36) NOT NULL,
  OBJECT NVARCHAR(100),
  EVENT NVARCHAR(40),
  DESTINATION NVARCHAR(1000),
  DESCRIPTION NVARCHAR(255),
  DESTINATION_TYPE NVARCHAR(50),
  CREATEDBY NVARCHAR(32),
  CREATEDAT TIME,
  CREATEDON DATE,
  CHANGEDBY NVARCHAR(32),
  CHANGEDAT TIME,
  CHANGEDON DATE,
  SOURCE NVARCHAR(10),
  SOURCE_REF_ID NVARCHAR(50),
  PRIMARY KEY(ID)
)