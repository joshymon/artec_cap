COLUMN TABLE "ARTEC_PC_UPLOAD_UPLD" (
  UPLD_GUID NVARCHAR(36) NOT NULL,
  AGGRID NVARCHAR(50),
  UPLD_REQ_ID NVARCHAR(36),
  UPLDTYPE NVARCHAR(20),
  PAYLOAD NCLOB,
  PROC_STATUS NVARCHAR(6),
  CREATEDBY NVARCHAR(32),
  CREATEDAT TIME,
  CREATEDON DATE,
  CHANGEDBY NVARCHAR(32),
  CHANGEDAT TIME,
  CHANGEDON DATE,
  SOURCE NVARCHAR(10),
  SOURCE_REF_ID NVARCHAR(50),
  PRIMARY KEY(UPLD_GUID)
)